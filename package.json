{
  "name": "hooks-demo",
  "version": "0.2.0",
  "private": true,
  "source": "src/index.html",
  "alias": {
    "components/**": "/src/components/$1",
    "data/**": "/src/data/$1",
    "hooks/**": "/src/hooks/$1",
    "services/**": "/src/services/$1",
    "styles/**": "/src/styles/$1"
  },
  "scripts": {
    "start": "concurrently \"parcel\" \"rescript build -w\"",
    "build": "rescript build && parcel build",
    "clean": "rescript clean -with-deps"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rescript/react": "^0.11.0",
    "concurrently": "^8.0.1",
    "parcel": "^2.8.3",
    "process": "^0.11.10",
    "rescript": "^10.1.4"
  },
  "browserslist": [
    "defaults",
    "not ie <= 11"
  ]
}
